{
  "name": "@modules/cosmwasm-cli",
  "private": true,
  "scripts": {
    "build": "pnpm run \"/^build:.*/\"",
    "build:darwin:amd64": "GOOS=darwin GOARCH=amd64 go build -o dist/darwin-amd64 .",
    "build:darwin:arm64": "GOOS=darwin GOARCH=arm64 go build -o dist/darwin-arm64 .",
    "build:linux:amd64": "GOOS=linux GOARCH=amd64 go build -o dist/linux-amd64 .",
    "build:linux:arm64": "GOOS=linux GOARCH=arm64 go build -o dist/linux-arm64 .",
    "build:windows:amd64": "GOOS=windows GOARCH=amd64 go build -o dist/windows-amd64 .",
    "build:windows:arm64": "GOOS=windows GOARCH=arm64 go build -o dist/windows-arm64 .",
    "test": "gotestsum -- ./... -p 6"
  }
}
