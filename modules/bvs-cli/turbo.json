{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "test": {
      "inputs": ["**.go"],
      "dependsOn": ["@modules/bvs-cw#generate:schema"]
    },
    "test:e2e": {
