{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "inputs": ["src", "tsconfig.json"],
      "outputs": ["dist"],
      "dependsOn": ["generate"]
    },
    "generate": {
      "inputs": ["generate.mjs"],
      "outputs": ["src"],
      "dependsOn": ["^build"]
    }
  }
}
