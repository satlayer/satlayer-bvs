{
  "name": "@examples/stablecoin-collateral-example",
  "private": false,
  "exports": {
    "./*": "./out/*"
  },
  "files": [
    "out"
  ],
  "scripts": {
    "build": "forge build",
    "docs": "mm-foundryup -b forge && node_modules/.bin/forge doc",
    "fmt": "forge fmt",
    "test": "forge test && vitest run --reporter verbose",
    "test:sol": "forge test",
    "test:ts": "vitest run --reporter verbose"
  },
  "lint-staged": {
    "!(*.sol)": [
      "prettier --write --ignore-unknown"
    ],
    "*.sol": [
      "forge fmt"
    ]
  },
  "dependencies": {
    "@aave/core-v3": "^1.19.3",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@satlayer/contracts": "workspace:*",
    "forge-std": "https://github.com/foundry-rs/forge-std/tarball/v1.9.7"
  },
  "devDependencies": {
    "@metamask/foundryup": "^1.0.1",
    "@openzeppelin/foundry-upgrades": "^0.4.0",
    "@satlayer/testcontainers": "^2.4.0",
    "@types/node": "^20.17.17",
    "abitype": "^1.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "viem": "^2.37.5",
    "vitest": "^3.2.4"
  }
}
